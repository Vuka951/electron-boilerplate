{
  "name": "minimal-react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal React Boilerplate",
  "main": "public/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "build": "webpack --mode production",
    "electron-start": "electron .",
    "electron-dev": "concurrently \"npm start\" \"wait-on http-get://localhost:8080 && npm run electron-start\"",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=public/main.js",
    "postinstall": "install-app-deps",
    "lint": "eslint src"
  },
  "build": {
    "asar": false,
    "appId": "TheAPPThing",
    "files": [
      "web-build/**/*",
      "./public/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "keywords": [],
  "homepage": "./",
  "author": "Vuka",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "autoprefixer": "^9.4.4",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "clean-webpack-plugin": "^0.1.19",
    "concurrently": "^4.1.0",
    "css-loader": "^1.0.1",
    "dotenv": "^6.2.0",
    "electron": "^4.0.1",
    "electron-builder": "^20.38.5",
    "eslint": "^5.12.0",
    "eslint-config-google": "^0.10.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-react": "^7.12.3",
    "file-loader": "^2.0.0",
    "glob": "^7.1.3",
    "html-webpack-plugin": "^3.2.0",
    "mini-css-extract-plugin": "^0.4.5",
    "node-sass": "^4.11.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "postcss-loader": "^3.0.0",
    "purgecss-webpack-plugin": "^1.4.0",
    "react-hot-loader": "^4.6.3",
    "redux-devtools-extension": "^2.13.7",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "terser-webpack-plugin": "^1.2.1",
    "url-loader": "^1.1.2",
    "wait-on": "^3.2.0",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.6.1",
    "axios": "^0.18.0",
    "bootstrap": "^4.2.1",
    "cors": "^2.8.5",
    "electron-is-dev": "^1.0.1",
    "express": "^4.16.4",
    "jquery": "^3.3.1",
    "knex": "^0.16.3",
    "popper.js": "^1.14.6",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-intl": "^2.7.2",
    "react-redux": "^5.1.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sqlite3": "^4.0.6"
  }
}
